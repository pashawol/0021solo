//- menu
mixin mnu()
	-
		var nav= {
			"HAUS UND GARTEN":"#",
			"PROFI":"#",
			"Wissenswertes":"#",
			"SERVICE":"#",
			"Unternehmen":"#",
		}
	+b.UL.navMenu&attributes(attributes)
		each linkPatch, linkName in nav
			//-item
			+e.LI.item( itemprop="item" )
				+e.A.link(href=linkPatch)=linkName
//- /menu